<?xml version="1.0" encoding="UTF-8" ?>

<taglib 
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
	version="2.1">
	<description>portal ui taglib</description>
	<tlib-version>0.1</tlib-version>
	<short-name>portal-ui-taglib</short-name>
	<uri>http://midaiganes.ee/portal-tags/ui</uri>

	<tag-file>
		<name>autogenerated-form</name>
		<path>/META-INF/tags/ui/form/autogenerated-form.tag</path>
	</tag-file>
	
	<tag-file>
		<name>form-content</name>
		<path>/META-INF/tags/ui/form/form-content.tag</path>
	</tag-file>
	<tag-file>
		<name>form-title</name>
		<path>/META-INF/tags/ui/form/form-title.tag</path>
	</tag-file>
	<tag-file>
		<name>form-input-row</name>
		<path>/META-INF/tags/ui/form/form-input-row.tag</path>
	</tag-file>
	<tag-file>
		<name>form-input-row-spring</name>
		<path>/META-INF/tags/ui/form/form-input-row-spring.tag</path>
	</tag-file>
	<tag-file>
		<name>form-select-row</name>
		<path>/META-INF/tags/ui/form/form-select-row.tag</path>
	</tag-file>
	<tag-file>
		<name>form-select-row-wrapper</name>
		<path>/META-INF/tags/ui/form/form-select-row-wrapper.tag</path>
	</tag-file>
	<tag-file>
		<name>form-textarea-row</name>
		<path>/META-INF/tags/ui/form/form-textarea-row.tag</path>
	</tag-file>
	<tag-file>
		<name>portlet-paginator</name>
		<path>/META-INF/tags/ui/portlet-paginator.tag</path>
	</tag-file>
</taglib>