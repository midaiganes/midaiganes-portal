			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>permissions</title>
			<short-title>permissions</short-title>
			<keywords>permissions</keywords>
		</portlet-info>
	</portlet>
	
	<portlet>
		<portlet-name>users-groups</portlet-name>
		<display-name>users-groups</display-name>
		<portlet-class>ee.midaiganes.portlets.usersgroups.UsersGroupsController</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>users-groups</title>
			<short-title>users-groups</short-title>
			<keywords>users-groups</keywords>
		</portlet-info>
	</portlet>
	
	<portlet>
		<portlet-name>user-profile</portlet-name>
		<display-name>user-profile</display-name>
		<portlet-class>ee.midaiganes.portlets.userprofile.UserProfilePortlet</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
		</supports>
		<portlet-info>
			<title>user-profile</title>
			<short-title>user-profile</short-title>
			<keywords>user-profile</keywords>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>chat</portlet-name>
		<display-name>chat</display-name>
		<portlet-class>ee.midaiganes.portlets.chat.ChatPortlet</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>chat</title>
			<short-title>chat</short-title>
			<keywords>chat</keywords>
		</portlet-info>
	</portlet>
</portlet-app>