<portlet-app 
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	version="2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	
	<portlet>
		<portlet-name>add-remove-portlet</portlet-name>
		<display-name>add-remove-portlet</display-name>
		<portlet-class>ee.midaiganes.portlets.addremoveportlet.AddRemovePortletController</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>add-remove-portlet</title>
			<short-title>add-remove-portlet</short-title>
			<keywords>add-remove-portlet</keywords>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>layout-set</portlet-name>
		<display-name>layout-set</display-name>
		<portlet-class>ee.midaiganes.portlets.layoutset.LayoutSetController</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
		</supports>
		<portlet-info>
			<title>layout-set</title>
			<short-title>layout-set</short-title>
			<keywords>layout-set</keywords>
		</portlet-info>
	</portlet>
	
	<portlet>
		<portlet-name>layout</portlet-name>
		<display-name>layout</display-name>
		<portlet-class>ee.midaiganes.portlets.layout.LayoutsController</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>layout</title>
			<short-title>layout</short-title>
			<keywords>layout</keywords>
		</portlet-info>
	</portlet>
	
	<portlet>
		<portlet-name>page-layout</portlet-name>
		<display-name>page-layout</display-name>
		<portlet-class>ee.midaiganes.portlets.pagelayout.PageLayoutController</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>page-layout</title>
			<short-title>page-layout</short-title>
			<keywords>page-layout</keywords>
		</portlet-info>
	</portlet>

	<portlet>
		<portlet-name>registration</portlet-name>
		<display-name>registration</display-name>
		<portlet-class>ee.midaiganes.portlets.registration.RegistrationController</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>registration</title>
			<short-title>registration</short-title>
			<keywords>registration</keywords>
		</portlet-info>
	</portlet>
	
	<portlet>
		<portlet-name>web-content</portlet-name>
		<display-name>web-content</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/portlet-context/web-content-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<window-state>normal</window-state>
		</supports>
		<portlet-info>
			<title>web-content</title>
			<short-title>web-content</short-title>
			<keywords>web-content</keywords>
		</portlet-info>
	</portlet>

	<portlet>
		<portlet-name>login</portlet-name>
		<display-name>login</display-name>
		<portlet-class>ee.midaiganes.portlets.login.LoginController</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<window-state>normal</window-state>
			<window-state>exclusive</window-state>
		</supports>
		<portlet-info>
			<title>login</title>
			<short-title>login</short-title>
			<keywords>login</keywords>
		</portlet-info>
	</portlet>
	
	<portlet>
		<portlet-name>permissions</portlet-name>
